{
  "name": "wanphp/composer-installers-extender",
  "description": "composer 安装wanphp扩展",
  "homepage": "http://www.wanphp.com/",
  "type": "composer-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "火子",
      "email": "wanphp@outlook.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "composer/composer": "^2.0",
    "composer-plugin-api": "^1.1 || ^2.0",
    "composer/installers": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Wanphp\\ComposerInstallersExtender\\": "src/"
    }
  },
  "extra": {
    "class": "Wanphp\\ComposerInstallersExtender\\Plugin"
  },
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
